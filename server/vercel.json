{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/user/(.*)", "dest": "/Routers/user-router.js" },
      { "src": "/products/(.*)", "dest": "/Routers/product-router.js" },
      { "src": "/(.*)", "dest": "/index.js" }
    ]
  }
  